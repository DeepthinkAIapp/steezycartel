{
  "version": 2,
  "public": true,
  "headers": [
    {
      "source": "/styles.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/$1"
        }
      ]
    },
    {
      "source": "/music/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/$1"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/styles.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1",
      "headers": {
        "Content-Type": "image/$1"
      }
    },
    {
      "src": "/music/(.*)",
      "dest": "/music/$1",
      "headers": {
        "Content-Type": "audio/$1"
      }
    }
  ]
} 